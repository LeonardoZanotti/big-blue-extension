{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Big Blue",
    "description": "Big Blue moaning extension",
    "author": "Leonardo Zanotti",
    "browser_action": {
   	 "default_title": "Big Blue",
   	 "default_popup": "index.html",
   	 "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}